{
  "name": "csgo-server",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "prettier-check": "prettier --check \"**/*.+(toml|vue|ts|js|tsx|jsx|cjs|json|css|md)\"",
    "prettier": "prettier --write \"**/*.+(toml|vue|ts|js|tsx|jsx|json|cjs|css|md)\"",
    "pretty": "npm run prettier && npm run lint-fix",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "tauri": "tauri"
  },
  "dependencies": {
    "@tauri-apps/api": "^1.2.0",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@tauri-apps/cli": "^1.2.3",
    "@types/node": "^18.15.11",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "eslint": "^8.38.0",
    "eslint-plugin-vue": "^9.10.0",
    "prettier": "^2.8.7",
    "prettier-plugin-toml": "^0.3.1",
    "typescript": "^5.0.4",
    "vite": "^4.2.1",
    "vue-tsc": "^1.2.0"
  }
}
